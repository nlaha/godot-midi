[configuration]

entry_symbol = "godotmidi_library_init"
compatibility_minimum = 4.2
android_aar_plugin = true

[libraries]

macos.debug = "res://addons/godot_midi/bin/macos/libgodotmidi.macos.template_debug.dev.framework"
macos.release = "res://addons/godot_midi/bin/macos/libgodotmidi.macos.template_release.framework"

windows.debug.x86_64 = "res://addons/godot_midi/bin/windows/libgodotmidi.windows.template_debug.dev.x86_64.dll"
windows.release.x86_64 = "res://addons/godot_midi/bin/windows/libgodotmidi.windows.template_release.x86_64.dll"

linux.debug.x86_64 = "res://addons/godot_midi/bin/linux/libgodotmidi.linux.template_debug.dev.x86_64.so"
linux.release.x86_64 = "res://addons/godot_midi/bin/linux/libgodotmidi.linux.template_release.x86_64.so"

linux.debug.arm64 = "res://addons/godot_midi/bin/linux/libgodotmidi.linux.template_debug.dev.arm64.so"
linux.release.arm64 = "res://addons/godot_midi/bin/linux/libgodotmidi.linux.template_release.arm64.so"

linux.debug.rv64 = "res://addons/godot_midi/bin/linux/libgodotmidi.linux.template_debug.dev.rv64.so"
linux.release.rv64 = "res://addons/godot_midi/bin/linux/libgodotmidi.linux.template_release.rv64.so"

android.debug.x86_64 = "res://addons/godot_midi/bin/release/x86_64/libGDExtensionGodotMidi.so"
android.release.x86_64 = "res://addons/godot_midi/bin/debug/x86_64/libGDExtensionGodotMidi.so"

android.debug.arm64 = "res://addons/godot_midi/bin/release/arm64-v8a/libGDExtensionGodotMidi.so"
android.release.arm64 = "res://addons/godot_midi/bin/debug/arm64-v8a/libGDExtensionGodotMidi.so"
